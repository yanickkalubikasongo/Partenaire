                    <div className="row">
                        <div className="col-sm-2">
                            <p className="mb-0">Prénom Nom Postnom</p>
                        </div>
                        <div className="col-sm-9">
                            <span id="email"><%=photo[0].prenom %> <%=photo[0].nom %> <%=photo[0].post_nom %></span>
                        </div>
                        <hr/>
                        <div className="row">
                            <div className="col-sm-3">
                                <p className="mb-0">Sexe</p>
                            </div>
                            <div className="col-sm-9">
                                <span id="tel"><%=photo[0].sexe %></span>
                            </div>
                        </div>
                        <hr/>
                        <div className="row">
                            <div className="col-sm-3">
                                <p className="mb-0">Email</p>
                            </div>
                            <div className="col-sm-9">
                                <input type="email" width="100px" id="adresse_electronique" value=<%=photo[0].email %> >
                            </div>
                        </div>
                        <hr/>
                        <div className="row">
                            <div className="col-sm-3">
